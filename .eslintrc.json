{
    "ignorePatterns": ["**/lib/*.js"],
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "no-case-declarations": "warn",
        "no-empty": "warn",
        "no-redeclare": "warn",
        "no-unreachable": "warn"
    }
}
